[Unit]
Description=Minecraft ICS SurvivalCraft Server
After=network.target

[Service]
Restart=always
StartLimitIntervalSec=10
StartLimitBurst=2
ExecStart=/root/PlumdoodleMCServer/startServer.sh
WorkingDirectory=/root/PlumdoodleMCServer
Type=simple
StandardError=syslog
NotifyAccess=all

[Install]
WantedBy=multi-user.target
